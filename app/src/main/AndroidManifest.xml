<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComposeUITest"
        tools:targetApi="31">
        <activity
            android:name=".CartScreenActivity"
            android:exported="false"
            android:label="@string/title_activity_cart_screen"
            android:theme="@style/Theme.ComposeUITest" />
        <activity
            android:name=".ConfirmPurchaseActivity"
            android:exported="false"
            android:label="@string/title_activity_confirm_purchase_screen"
            android:theme="@style/Theme.ComposeUITest" />
        <activity
            android:name=".AddressScreenActivity"
            android:exported="false"
            android:label="@string/title_activity_address_screen"
            android:theme="@style/Theme.ComposeUITest" />
        <activity
            android:name=".ItemScreenActivity"
            android:exported="false"
            android:label="@string/title_activity_item_screen"
            android:theme="@style/Theme.ComposeUITest" />
        <activity
            android:name=".ShowcaseActivity"
            android:exported="false"
            android:label="@string/title_activity_showcase"
            android:theme="@style/Theme.ComposeUITest" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ComposeUITest">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>